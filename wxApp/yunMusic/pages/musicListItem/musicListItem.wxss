.musicListItem{
  display: flex;
  flex-direction: column;
  width: 250rpx;
}
image{
  width:200rpx ;
  height:200rpx ;
  border-radius: 10px;
  margin-left: 25rpx;
  margin-top: 15rpx;
}
.info{
  color: #7f7f7f;
  font-size: 26rpx;
  margin: 10rpx 25rpx;
  -webkit-line-clamp:2;           
  display: -webkit-box;          
  -webkit-box-orient:vertical;   
  overflow:hidden;            
  text-overflow: ellipsis;       
}